{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "flight-recorder-rust",
  "version": "0.0.0",
  "identifier": "FlightRecorder",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "flight-recorder-rust",
        "width": 900,
        "height": 900,
        "minWidth": 900,
        "minHeight": 900
      }
    ],
    "security": {
      "csp": null
    }
  }
}
